{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "game": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::game.game"
    },
    "slug": {
      "type": "string"
    },
    "pointReward": {
      "type": "integer",
      "required": true
    },
    "itemReward": {
      "type": "integer",
      "default": 1
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "url": {
      "type": "string"
    },
    "interval": {
      "type": "integer"
    },
    "startTime": {
      "type": "datetime"
    },
    "endTime": {
      "type": "datetime"
    },
    "task_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::task-category.task-category"
    },
    "active": {
      "type": "boolean"
    },
    "onChainType": {
      "type": "enumeration",
      "enum": [
        "attendance"
      ]
    },
    "network": {
      "type": "enumeration",
      "enum": [
        "karura",
        "acala",
        "vara_network",
        "kusama",
        "polkadot",
        "alephTest",
        "creditcoin",
        "creditcoinTest"
      ]
    },
    "share_leaderboard": {
      "type": "component",
      "repeatable": false,
      "component": "task.content-share"
    },
    "airlyftId": {
      "type": "string"
    },
    "airlyftType": {
      "type": "enumeration",
      "enum": [
        "normal",
        "sync",
        "telegram-sync"
      ],
      "default": "normal"
    },
    "airlyftEventId": {
      "type": "string"
    },
    "airlyftWidgetId": {
      "type": "string"
    },
    "achievement": {
      "type": "component",
      "repeatable": false,
      "component": "task.achievement"
    }
  }
}
