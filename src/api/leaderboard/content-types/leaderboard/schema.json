{
  "kind": "collectionType",
  "collectionName": "leaderboards",
  "info": {
    "singularName": "leaderboard",
    "pluralName": "leaderboards",
    "displayName": "Leaderboard",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "all:nps",
        "task:nps",
        "task:quantity",
        "referral:nps",
        "referral:quantity",
        "referral:inviteToPlay:nps",
        "game:casual:nps",
        "game:casual:point",
        "game:casual:quantity",
        "game:farming:point",
        "game:farming:totalPoint",
        "game:farming:earnSpeed"
      ]
    },
    "specialTime": {
      "type": "enumeration",
      "enum": [
        "weekly",
        "monthly",
        "yearly"
      ]
    },
    "startTime": {
      "type": "datetime"
    },
    "endTime": {
      "type": "datetime"
    },
    "games": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::game.game"
    },
    "tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::task.task"
    },
    "metadata": {
      "type": "json"
    },
    "sharing": {
      "type": "component",
      "repeatable": false,
      "component": "leader-board.x-sharing"
    }
  }
}
