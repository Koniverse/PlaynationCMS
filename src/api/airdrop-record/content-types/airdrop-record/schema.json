{
  "kind": "collectionType",
  "collectionName": "airdrop_records",
  "info": {
    "singularName": "airdrop-record",
    "pluralName": "airdrop-records",
    "displayName": "Airdrop Record",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "campaign_id": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::airdrop-campaign.airdrop-campaign"
    },
    "account_id": {
      "type": "integer"
    },
    "amount": {
      "type": "integer"
    },
    "network": {
      "type": "string",
      "default": "KAR"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "NEW_REGISTRATION",
        "CHECKING_CONDITIONS",
        "PASS_CONDITION_CHECK",
        "ELIGIBLE_FOR_REWARD",
        "RECEIVED",
        "NOT_ELIGIBLE_FOR_REWARD",
        "CANCELED"
      ]
    },
    "snapshot_data": {
      "type": "richtext"
    },
    "point": {
      "type": "integer"
    }
  }
}
